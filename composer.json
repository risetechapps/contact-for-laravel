{
    "name": "risetechapps/contact-for-laravel",
    "description": "",
    "keywords": [
        "risetechapps",
        "contacts",
        "tenancy",
        "laravel"
    ],
    "homepage": "https://github.com/risetechapps/contact-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "risetechapps/has-uuid-for-laravel": "^1.0.0",
        "risetechapps/to-upper-for-laravel": "^1.0.0",
        "risetechapps/monitoring": "^1.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\Contact\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\Contact\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\Contact\\ContactServiceProvider"
            ],
            "aliases": {
                "Contact": "RiseTechApps\\Contact\\ContactFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0"
}
